{
    "compilerOptions": {
      "target": "esnext",
      "module": "CommonJS",
      "esModuleInterop": true,
      "strict": true,
      "moduleResolution": "node",
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  